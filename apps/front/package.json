{
  "name": "friday-web-app",
  "version": "1.0.0",
  "description": "Friday web app",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:prod": "cross-env NODE_ENV=production http-server dist -p 1444 -g",
    "start:dev": "cross-env NODE_ENV=development vite --host --force",
    "start:demo": "cross-env NODE_ENV=production tsc && vite build --mode staging && http-server dist",
    "build": "cross-env NODE_ENV=production tsc && vite build --mode production",
    "preview": "pnpm build && vite preview --host",
    "preview:https": "cross-env HTTPS=true pnpm run preview",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src"
  },
  "prettier": "../../packages/tools/prettier.config.js",
  "dependencies": {
    "@ebay/nice-modal-react": "^1.2.10",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@friday-ai/shared": "workspace:*",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@visx/event": "^3.3.0",
    "@visx/group": "^3.3.0",
    "@visx/legend": "^3.3.0",
    "@visx/responsive": "^3.3.0",
    "@visx/scale": "^3.3.0",
    "@visx/shape": "^3.3.0",
    "@visx/tooltip": "^3.3.0",
    "axios": "^1.6.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.1",
    "http-server": "^14.1.1",
    "i18next": "^23.4.4",
    "i18next-browser-languagedetector": "^7.1.0",
    "leaflet": "^1.9.4",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.1.2",
    "react-leaflet": "^4.2.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.15.0",
    "use-between": "^1.3.5"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base-typescript-prettier": "^5.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "prettier": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-svgr": "^3.2.0",
    "workbox-window": "^7.0.0"
  }
}
