{
  "name": "friday-web-app",
  "version": "1.0.0",
  "description": "Friday web app",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:prod": "cross-env NODE_ENV=production http-server dist -p 1444",
    "start:dev": "cross-env NODE_ENV=development vite --host",
    "start:demo": "cross-env NODE_ENV=production tsc && vite build --mode staging && http-server dist",
    "build": "cross-env NODE_ENV=production tsc && vite build --mode production",
    "preview": "pnpm build && vite preview --host",
    "preview:https": "cross-env HTTPS=true pnpm run preview",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src"
  },
  "prettier": "../../packages/tools/prettier.config.js",
  "dependencies": {
    "@iconify/react": "^4.0.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/react-redux": "^7.1.25",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^9.0.0",
    "axios": "^1.2.2",
    "cross-env": "^7.0.3",
    "daisyui": "^2.46.1",
    "framer-motion": "^8.2.4",
    "http-server": "^14.1.1",
    "leaflet": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-leaflet": "^4.2.0",
    "react-redux": "^8.0.5",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.6.1",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.0",
    "tailwindcss": "^3.2.4",
    "use-between": "^1.3.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-pwa": "^0.14.1",
    "workbox-window": "^6.5.4"
  }
}
