{
  "name": "friday-web-app",
  "version": "1.0.0",
  "description": "Friday web app",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "per-env",
    "start:prod": "cross-env NODE_ENV=production http-server dist -p 1444 -g",
    "start:dev": "cross-env NODE_ENV=development vite --host --force",
    "start:demo": "cross-env NODE_ENV=production tsc && vite build --mode staging && http-server dist",
    "build": "cross-env NODE_ENV=production tsc && vite build --mode production",
    "preview": "pnpm build && vite preview --host",
    "preview:https": "cross-env HTTPS=true pnpm run preview",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src"
  },
  "prettier": "../../packages/tools/prettier.config.js",
  "dependencies": {
    "@ebay/nice-modal-react": "^1.2.13",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@friday-ai/shared": "workspace:*",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@tanstack/react-query": "^5.17.8",
    "@visx/event": "^3.3.0",
    "@visx/group": "^3.3.0",
    "@visx/legend": "^3.5.0",
    "@visx/responsive": "^3.3.0",
    "@visx/scale": "^3.5.0",
    "@visx/shape": "^3.5.0",
    "@visx/tooltip": "^3.3.0",
    "axios": "^1.6.5",
    "cross-env": "^7.0.3",
    "date-fns": "^3.1.0",
    "framer-motion": "^10.17.9",
    "http-server": "^14.1.1",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "leaflet": "^1.9.4",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.21.1",
    "use-between": "^1.3.5",
    "useragent-parser-js": "^1.0.3"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base-typescript-prettier": "^5.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-svgr": "^4.2.0",
    "workbox-window": "^7.0.0"
  }
}
