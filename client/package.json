{
  "name": "friday-web-app",
  "version": "1.0.0",
  "description": "Friday web app",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:prod": "npm run -s serve",
    "start:dev": "npm run -s dev",
    "start:demo": "npm run -s demo",
    "dev": "cross-env NODE_ENV=development vite --host",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "preview": "npm run build && vite preview --host",
    "preview:https": "cross-env HTTPS=true npm run preview",
    "serve": "cross-env NODE_ENV=production npm run build && serve dist",
    "demo": "cross-env NODE_ENV=production tsc && vite build --mode staging && serve dist",
    "test": "echo \"Error: tests are not yet implemented\" && exit 1",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "concurrently -k -n \"ESLint\" -c \"blue\" \"eslint --ext .js,.jsx,.ts,.tsx ./src\"",
    "lint:fix": "concurrently -k -n \"ESLint\" -c \"blue\" \"eslint --ext .js,.jsx,.ts,.tsx --fix ./src\""
  },
  "dependencies": {
    "@iconify/react": "^3.2.2",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/react-redux": "^7.1.24",
    "@types/react-transition-group": "^4.4.4",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "daisyui": "^2.15.2",
    "framer-motion": "^6.3.10",
    "leaflet": "^1.8.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hot-toast": "^2.2.0",
    "react-leaflet": "^4.0.0",
    "react-multi-date-picker": "^3.3.0",
    "react-redux": "^8.0.2",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.2.0",
    "serve": "^13.0.2",
    "tailwindcss": "^3.0.24",
    "use-between": "^1.3.4",
    "uuid": "^8.3.2",
    "workbox-window": "^6.5.3"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.10",
    "@types/react": "^18.0.11",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.9",
    "vite-plugin-pwa": "^0.12.0"
  },
  "resolutions": {
    "postcss": "8.1.10"
  }
}
